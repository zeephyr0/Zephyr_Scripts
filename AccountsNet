async def stop_sending_messages(event):
    global stop_sending1
    stop_sending1 = True
    await event.message.edit('‚ùå–û—Ç–ø—Ä–∞–≤–∫–∞ —Å–æ–æ–±—â–µ–Ω–∏–π –≤—ã–∫–ª—é—á–µ–Ω–∞')

