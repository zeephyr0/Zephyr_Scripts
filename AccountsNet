# –ü—Ä–æ–º–æ-–ö–æ–¥—ã
@client1.on(events.NewMessage(chats=[mineEvo_channel, my_channel]))  
async def process_new_message(event):
    # –°–æ–æ–±—â–µ–Ω–∏–µ –∫–æ—Ç–æ—Ä–æ–µ –±—É–¥–µ—Ç –ø—Ä–æ–≤–µ—Ä—è—Ç—å—Å—è 
    message = event.message
    if '–ü—Ä–æ–º–æ' in message.message:
        # –û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –≤—ã–≤–æ–¥ –∏ –æ—Ç–ø—Ä–∞–≤–∫–∞ –ø—Ä–æ–º–æ–∫–æ–¥–∞
        promo_code = message.message.split('–ü—Ä–æ–º–æ ')[1].split()[0]
        await client1.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client2.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client3.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client4.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client5.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client6.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client7.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client8.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client9.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client10.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client11.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client12.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client13.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client14.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client15.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client16.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client17.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        await client19.send_message(mine_evo, f'–ø—Ä–æ–º–æ {promo_code}')
        
